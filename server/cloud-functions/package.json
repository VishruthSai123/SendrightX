{
  "name": "sendright-google-play-webhooks",
  "version": "1.0.0",
  "description": "Google Play Developer API webhook handlers for SendRight",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
